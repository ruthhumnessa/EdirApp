
@model WebApplication4.ViewModels.RandomEdirViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Edir.Name</h2>

@if (Model.Customers.Count == 0)
{
    <p>No one has joined this edir yet. </p>

}
<ul>
    @foreach (var customer in Model.Customers)
    {
        <li>@customer.Name</li>
    }
</ul>
